{
  "name": "regdem2015",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:css & npm run build:js",
    "build:css": "lessc css/core.css static/bundle.css && npm run preprocess:autoprefixer; true",
    "build:js": "browserify js/app.js -o static/bundle.js",
    "watch": "npm run watch:css & npm run watch:js & npm run watchqa:js",
    "watch:css": "nodemon -I -w css/ --ext css --exec 'npm run build:css'",
    "watch:js": "watchify js/app.js -o static/bundle.js -v",
    "watchqa:js": "nodemon -q -I -w js --ext js,jsx --exec 'npm run qa'",
    "preprocess:autoprefixer": "autoprefixer static/bundle.css",
    "qa": "eslint js/**.js*; true",
    "test": "mocha test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nvdb-vegdata/regdem2015.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nvdb-vegdata/regdem2015/issues"
  },
  "homepage": "https://github.com/nvdb-vegdata/regdem2015#readme",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^5.1.1",
    "babelify": "^6.1.0",
    "browserify": "^10.2.0",
    "eslint": "^0.23.0",
    "eslint-plugin-react": "^2.5.2",
    "mocha": "^2.2.4",
    "nodemon": "^1.3.7",
    "react": "^0.13.3",
    "watchify": "^3.2.1"
  },
  "dependencies": {
    "leaflet-markercluster": "^0.2.0",
    "leaflet-omnivore": "^0.3.2",
    "less": "^2.5.1",
    "qwest": "^1.7.0",
    "react-select": "^0.4.9",
    "react-typeahead-component": "^0.6.0",
    "material-ui": "^0.9.2",
    "react-tap-event-plugin": "^0.1.7"
  }
}
